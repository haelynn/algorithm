#include <iostream>

using namespace std;

int arr[1001];

void swap(int *a, int *b){
    int *tmp = a;
    a = b;
    b = tmp;
}

void quickSort(int l, int r){

    if(r <= l) return;

    int i = l;
    int j = r + 1;
    int p = l;

    if(arr[l] < arr[r]) p = r;
    if(arr[l + (r - l) / 2] < arr[p]) p = l + (r - l) / 2;

    swap(arr[l], arr[p]);
    while(i < j){
        do{ i++; } while(arr[l] > arr[i] && i <= r);
        do{ j--; } while(arr[l] < arr[j] && j >= l+1);
        if(i < j) swap(arr[i], arr[j]);
    }

    swap(arr[l], arr[j]);
    quickSort(l, j-1);
    quickSort(j+1, r);
}

int main(){

    int n;
    cin >> n;

    for(int i=0; i<n; i++){
        cin >> arr[i];
    }

    quickSort(0, n-1);

    for(int i=0; i<n; i++)
        cout << arr[i] << " ";

}
