#include <vector>
using namespace std;
 
//핵심 소스코드의 설명을 주석으로 작성하면 평가에 큰 도움이 됩니다.
class Solution{
public:
    int solution(vector<int> marks){
        
        int sum = 0;
        int mSize = marks.size();
        for(int i=0; i<mSize; i++) sum+=marks[i];
        
        while(true){
            sum += 10;
            mSize++;
            
            if((double)sum/mSize >= 9.5) return mSize - marks.size();
        }
        
        return 0;
    }
};
