#include <vector>
#include <algorithm>
 
using namespace std;
 
int d[51] = {0, };
int cnt[51] = {0, };
 
//핵심 소스코드의 설명을 주석으로 작성하면 평가에 큰 도움이 됩니다.
class Solution{
public:
    int solution(vector<int> type){
        
    for(int i=0; i<type.size(); i++){
        cnt[type[i]]++;
    }
 
    d[1] = cnt[1];
    for(int i=2; i<=type.size(); i++){
        d[i] = d[i-1] * cnt[i];
    }
    
    int res = 0;
    for(int i=1; i<=type.size(); i++) res += d[i];
        
        return res;
    }
};
