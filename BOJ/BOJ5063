#include <iostream>
#include <algorithm>

using namespace std;

int main(){

    ios::sync_with_stdio(false);
    cin.tie(0);

    int t;
    cin >> t;

    int r, e, c;

    while(t--){

        cin >> r >> e >> c;

        if(r == e-c) cout << "does not matter\n";
        else if(r < e-c) cout << "advertise\n";
        else cout << "do not advertise\n";
    }
}
