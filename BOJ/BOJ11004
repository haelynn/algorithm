#include <algorithm>
#include <iostream>
#include <vector>

using namespace std;

int main(){
    int n, k;
    scanf("%d %d", &n, &k);

    vector<int> arr;
    for(int i=0; i<n; i++){
        int tmp;
        scanf("%d", &tmp);

        arr.push_back(tmp);
    }

    sort(arr.begin(), arr.end());
    printf("%d\n", arr[k-1]);
}
